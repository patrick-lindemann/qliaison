{
  "name": "@qliaison/ag-grid",
  "version": "0.0.11",
  "description": "The AG Grid filter parser module for qLiaison.",
  "author": "Patrick Lindemann",
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "filter",
    "connector",
    "ag-grid"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/patrick-lindemann/qliaison",
    "directory": "packages/ag-grid"
  },
  "bugs": {
    "url": "https://github.com/patrick-lindemann/qliaison/issues"
  },
  "scripts": {
    "clean": "rimraf node_modules lib *.tsbuildinfo",
    "build": "tsc --build",
    "build:dev": "tsc --build --sourceMap",
    "test": "jest --config ../../jest.config.js --rootDir . --passWithNoTests",
    "lint": "eslint ./src/**/*.ts --config ../../.eslintrc --fix",
    "format": "prettier './src/**/*.ts' --config ../../.prettierrc --write"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@qliaison/core": "workspace:*",
    "ag-grid-community": "^32.0.2",
    "ag-grid-enterprise": "^32.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
